{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "blue": "#00C5C7",
    "green": "#33FF57",
    "os": "#ACB0BE",
    "pink": "#F4B8E4",
    "red": "#FF5733"
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "style": "plain",
          "template": "{{.Icon}} ",
          "foreground": "p:os",
          "type": "os"
        },
        {
          "properties": {
            "folder_icon": "..\ue5fe..",
            "home_icon": "~",
            "style": "agnoster_short"
          },
          "style": "plain",
          "template": "{{ .Path }} ",
          "foreground": "p:blue",
          "type": "path"
        },
        {
          "properties": {
            "branch_icon": "\ue725 ",
            "cherry_pick_icon": "\ue29b ",
            "commit_icon": "\uf417 ",
            "fetch_status": true,
            "fetch_upstream_icon": false,
            "merge_icon": "\ue727 ",
            "no_commits_icon": "\uf0c3 ",
            "rebase_icon": "\ue728 ",
            "revert_icon": "\uf0e2 ",
            "tag_icon": "\uf412 "
          },
          "style": "plain",
          "template": "{{ .HEAD }}{{ if .Working.Changed }}<#9c9c9c>*</>{{ end }} {{ if gt .Ahead 0 }}<#FF0000>\ue353</>{{ end }}{{ if gt .Behind 0 }}<#00FFD0>\ue340</>{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ end }}",
          "foreground": "#bab02a",
          "type": "git"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "threshold": 10
          },
          "style": "plain",
          "template": "took \uf252 {{ .FormattedMs }} ",
          "foreground": "#bab02a",
          "type": "executiontime"
        },
        {
          "properties": {
            "time_format": "15:04:05"
          },
          "style": "plain",
          "template": " {{ .CurrentDate | date .Format }} \uf017 ",
          "foreground": "#00C5C7",
          "type": "time"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "always_enabled": true
          },
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "‚ùØ",
          "foreground": "#33FF57",
          "background": "#29315A",
          "type": "status",
          "foreground_templates": ["{{ if .Error }}#FF5733{{ end }}"]
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "final_space": true
}
